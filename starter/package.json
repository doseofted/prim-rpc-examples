{
	"name": "prim-rpc-starter",
	"version": "0.0.1",
	"packageManager": "pnpm@7.14.2",
	"description": "An example project to get started with Prim+RPC",
	"private": true,
	"license": "UNLICENSED",
	"type": "module",
	"scripts": {
		"dev:server": "tsx watch --clear-screen=false server.ts",
		"dev:client": "vite --port 3000",
		"dev": "concurrently \"npm:dev:*\"",
		"build": "vite build --target=esnext",
		"start": "vite build && tsx server.ts"
	},
	"devDependencies": {
		"concurrently": "^8.2.0",
		"solid-js": "^1.7.6",
		"typescript": "^5.0.4",
		"vite": "^4.3.9"
	},
	"dependencies": {
		"@fastify/cors": "^8.3.0",
		"@fastify/static": "^6.10.2",
		"fastify": "^4.18.0",
		"tsx": "^3.12.7"
	}
}
